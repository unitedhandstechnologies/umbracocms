@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.Home>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
    Layout = null;
    var slides = (ContentModels.SlideList)Model.Root().Children().Where(x =>
    x.IsDocumentType("slideList")).FirstOrDefault();
    var virticalslides = (ContentModels.VerticalSlideList)Model.Root().Children().Where(x =>
    x.IsDocumentType("verticalSlideList")).FirstOrDefault();
    var bucketList = (ContentModels.BucketListHotspots)Model.Root().Children().Where(x =>
    x.IsDocumentType("bucketListHotspots")).FirstOrDefault();
    var running = (ContentModels.RunningSlideList)Model.Root().Children().Where(x =>
    x.IsDocumentType("runningSlideList")).FirstOrDefault();
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <link rel="icon" type="image/x-icon" href="~/assets/images/favicon.ico" />

    <meta name="viewport"
        content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1, user-scalable=no" />
    <!-- bootstrap css file -->
    <link rel="stylesheet" href="~/assets/css/bootstrap.min.css" />
    <!-- all.min.css file -->
    <link rel="stylesheet" href="~/assets/css/all.min.css" />
    <!-- font-awesome css file -->

    <!-- style.css file -->
