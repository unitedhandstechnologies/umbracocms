                                href="https://www.youtube.com/watch?v=rahRaVtEQaM">
                                Watch Video
                                <div class="icon"></div>
                            </a>
                            <!--  -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--  -->

    <section class="lot_more_style mb-0">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <h1>@Model?.PartTitle1<span>@Model?.PartSubTitle</span></h1>
                </div>
            </div>
            <div class="row animatedParent">
                <div class="col-md-4 col-12 left_content_wrp animated fadeInUpShort">
                    <h4>@Model?.PartSudoTitle</h4>
                    @Model?.PartTitle2
                    <br>
                    <br>
                    @Model?.PartTitle3
                </div>
                <div class="col-md-7 offset-md-1 col-12 right_slider_wrp animated fadeInRightShort">
                    <div class="Home_offer_slider">
                        @{
                            foreach (ContentModels.Slide item in slides.Children())
                            {
                                <div class="slide">
                                    <div class="image_style">
                                        <div class="caption">
                                            <h3>@item.SlideTitle</h3>
                                            <p>@item.SlideDescription</p>
                                        </div>
                                        <img src=@item.SlideImage.Url() />
                                    </div>
                                </div>
                            }
                        }
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!--  -->

    <!-- Vertical Slider -->
    <section>
        <div class="container-fuild">
            <div class="row">
                <div class="col-12">
                    <div class="Slide_wrp">
                        <div class="slides">
                            @{
                                foreach (ContentModels.VirticalSlide item in virticalslides.Children())
                                {
                                    <div class="slide" style="background-image: url(@item.VerticalSlideImage.Url());">
                                        <div class="caption_style">
                                            <h1>@item.VerticalSlideTitle</h1>
                                            <h2>@item.VerticalSlideDescription</h2>
                                            <div class="slide_btn">
                                                <a href="#" tabindex="0"> Know More <i class="fa fa-long-arrow-right"></i> </a>
                                            </div>
                                        </div>
                                    </div>
